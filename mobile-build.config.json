{
  "name": "In The Club We All Fam",
  "slug": "club-fam",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./public/icon-512.png",
  "userInterfaceStyle": "light",
  "splash": {
    "image": "./public/icon-512.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "assetBundlePatterns": [
    "**/*"
